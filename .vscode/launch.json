{
    "version": "0.2.0",
    "configurations": [
      
      {
        "name": "Python: 调试当前文件",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "cwd": "${fileDirname}",
        "console": "integratedTerminal",
        "justMyCode": false,
        "stopOnEntry": false,
        "debugOptions": [
          "DebugStdLib"
        ]
      },
      {
        "name": "Node.js: 调试当前文件",
        "type": "node",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "stopOnEntry": false,
        "internalConsoleOptions": "openOnSessionStart"
      }
    ]
  }